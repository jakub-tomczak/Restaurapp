<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.112-ea">
    <children>
        <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                  prefHeight="400.0" prefWidth="600.0">
            <columnConstraints>
                <ColumnConstraints/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Nazwa firmy" GridPane.columnIndex="1"/>
                <Label text="REGON" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="Indywidualny numer konta" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField fx:id="corporateNameField" GridPane.columnIndex="2"/>
                <TextField fx:id="corporateREGONField" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <TextField fx:id="corporateBankAccountNumberField" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
            </children>
        </GridPane>
        <Button fx:id="saveCorporateButton" mnemonicParsing="false" text="Zapisz"/>
    </children>
    <padding>
        <Insets bottom="100.0" left="100.0" right="100.0" top="100.0"/>
    </padding>
</VBox>
